SQL_DOWN = u"""
DROP TABLE `cc`;
CREATE TABLE `zz` (
  `id` int(11) NOT NULL,
  `name` varchar(13) CHARACTER SET latin1 COLLATE latin1_german1_ci NOT NULL DEFAULT 'lol',
  `time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `bool` tinyint(1) NOT NULL,
  PRIMARY KEY (`id`,`name`),
  UNIQUE KEY `name` (`name`,`time`),
  KEY `time` (`time`,`bool`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;
CREATE ALGORITHM=UNDEFINED DEFINER=`root`@`%` SQL SECURITY DEFINER VIEW `myview` AS select `zz`.`id` AS `id`,`zz`.`name` AS `name`,`zz`.`time` AS `time`,`zz`.`bool` AS `bool` from `zz`;
ALTER TABLE `aa` ENGINE = MyISAM, DEFAULT COLLATE latin1_spanish_ci, DROP `as`, DROP `qw`, CHANGE `name` `name` varchar(255) CHARACTER SET latin1 NOT NULL DEFAULT 'aa', CHANGE `pass` `pass` varchar(255) CHARACTER SET latin1 NOT NULL, ADD `zx` int(11) NOT NULL, DROP INDEX `as`, DROP INDEX `name`, DROP INDEX `id`, ADD PRIMARY KEY (`id`,`name`), ADD UNIQUE KEY `pass` (`pass`), ADD UNIQUE KEY `namekey` (`name`,`pass`,`zx`), DROP FOREIGN KEY `as`;
ALTER TABLE `bb` DEFAULT COLLATE latin1_bin, DROP `jj`, DROP INDEX `id`;
ALTER TABLE `asas` DEFAULT COLLATE latin1_bin, CHANGE `lastname` `lastname` varchar(255) COLLATE latin1_bin NOT NULL, CHANGE `firstname` `firstname` varchar(255) COLLATE latin1_bin NOT NULL;
